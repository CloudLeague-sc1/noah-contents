{
 "env": {
   "browser": false,
   "es6": true
 },
 "extends": [
  "eslint:recommended",
  "plugin:@typescript-eslint/eslint-recommended",
  "plugin:@typescript-eslint/recommended",
  "prettier",
  "plugin:prettier/recommended"],
 "rules": {
   "prettier/prettier": [
     "error",
     {
       "singleQuote": true,
       "trailingComma": "es5"
     }
   ]
 },
 "parserOptions": {
  "ecmaVersion": 6,
  "sourceType": "module"
 }
}
